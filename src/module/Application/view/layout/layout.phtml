<script>
    $(function () {
        $('[data-toggle="popover"]').popover({ html: true});
    });
    $(function () {
        $('[data-bs-toggle="tooltip"]').tooltip({});


    $(function () {

        let demo = false
        let imgRole = document.querySelector('#roleImg');
        if(demo) {
            let imgGestionnaire = "data:image/jpeg;base64,"
            let imgEtudiant = "data:image/jpeg;base64,"
            let imgAdmin = "data:image/jpeg;base64,"

            let elemRole = document.querySelector('.role-libelle');
            if(elemRole) {
                let role = elemRole.innerHTML


                switch (role) {
                    case 'Administrateur technique':
                        imgRole.src = imgAdmin
                        break;
                    case 'Administrateur fonctionnel':
                        imgRole.src = imgAdmin
                        break;
                    case 'Etudiant':
                        imgRole.src = imgEtudiant
                        break;
                    case 'Gestionnaire':
                        imgRole.src = imgGestionnaire
                        break;
                    default:
                        imgRole.src = ''
                }
            }
        }else {
            imgRole.style.display = 'none'
        }

    });

    });
</script>

